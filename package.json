{
    "name": "dakara-navigator",
    "displayName": "Navigator",
    "description": "Enhanced cursor navigation and movement.",
    "version": "1.0.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/dakaraphi/vscode-extension-navigator"
    },
    "publisher": "dakara",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:navigator.fixedSpaceUp",
        "onCommand:navigator.fixedSpaceDown",
        "onCommand:navigator.wordRightEnd",
        "onCommand:navigator.wordLeftBegin"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "navigator.fixedSpaceUp",
                "title": "Navigator: Cursor jump fixed space up"
            },
            {
                "command": "navigator.fixedSpaceDown",
                "title": "Navigator: Cursor jump fixed space down"
            },
            {
                "command": "navigator.wordRightEnd",
                "title": "Navigator: Cursor word right until end"
            },
            {
                "command": "navigator.wordLeftBegin",
                "title": "Navigator: Cursor word left until begin"
            }
        ]
    },
    "scripts": {
        "__vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "vscode-extension-common": "file:../vscode-extension-common"
    }
}
